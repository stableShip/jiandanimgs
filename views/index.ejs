<!DOCTYPE html>
<html>

<head>
    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <title>OOXX-<%= pageId %></title>
    <link type="text/css" rel="stylesheet" href="/css/style.css" media="screen,projection" />
</head>
<body>
  <ul class="pagination">
    <% pageTurning.forEach(function(item) { %>
      <li class="<%= item.className %>"><a href="<%= item.url %>"><%= item.index %></a></li>
    <% }); %>
  </ul>
    <div id="container">
        <div id="content">
        <% list.forEach(function(item) { %>
          <ul>
          <% item.forEach(function(obj) { %>
              <li data-index="<%= obj.index %>">
                  <img src="<%= obj.url %>" >
                  <p><%= obj.text %></p>
              </li>
          <% }) %>
          </ul>
        <% }); %>
        </div>
    </div>
  <ul class="pagination">
    <% pageTurning.forEach(function(item) { %>
      <li class="<%= item.className %>"><a href="<%= item.url %>"><%= item.index %></a></li>
    <% }); %>
  </ul>
    <div id="popWindow">
        <div id="imgArea">
            <div class="c-wrap">
                <div class="c-inner"><img></div>
            </div>
        </div>
        <div id="contextmenu">
            <div class="menu-item" id="prev">上一张</div>
            <div class="menu-item" id="next">下一张</div>
            <div class="menu-item" id="closer">关闭</div>
        </div>
    </div>

    <script type="text/javascript" data-main="app" src="/lib/require.js"></script>
    <script type="text/javascript">
    require.config({
        "baseUrl": "/js",
        "paths": {
            'jquery': '../lib/jquery-2.1.1.min'
        }
    })
    </script>
</body>

</html>
